@{
    Layout = null;
}


    <link rel="stylesheet" type="text/css" href="https://horner.github.io/pivottable/dist/pivot.css">
<script type="text/javascript" src="https://horner.github.io/pivottable/examples/ext/d3.v3.min.js"></script>
<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript" src="https://horner.github.io/pivottable/examples/ext/jquery-1.8.3.min.js"></script>
<script type="text/javascript" src="https://horner.github.io/pivottable/examples/ext/jquery-ui-1.9.2.custom.min.js"></script>
<script type="text/javascript" src="~/Content/dist/pivot.js"></script>
<script type="text/javascript" src="~/Content/dist/gchart_renderers.js"></script>
<script type="text/javascript" src="~/Content/dist/d3_renderers.js"></script>
<script type="text/javascript" src="https://horner.github.io/pivottable/examples/ext/jquery.ui.touch-punch.min.js"></script>
<style>
    * {
        font-family: Verdana;
    }

    .node {
        border: solid 1px white;
        font: 10px sans-serif;
        line-height: 12px;
        overflow: hidden;
        position: absolute;
        text-indent: 2px;
    }
</style>

<script type="text/javascript">
            google.load("visualization", "1", {packages:["corechart", "charteditor"]});
            $(function(){
                $.getJSON('@Url.Action("loadLeadData")', function (mps) {
                    $("#output").pivotUI(mps, {
                        renderers: $.extend(
                            $.pivotUtilities.renderers,
                            $.pivotUtilities.gchart_renderers,
                            $.pivotUtilities.d3_renderers
                            ),
                        derivedAttributes: {},
                        rendererName: "Table"
                    });
                });
             });
</script>
<div id="output" style="margin: 30px;"></div>